@model IEnumerable<YJK2237A5.Models.GenreBaseViewModel>


<h2>Genre List</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.name)
        </th>
        
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.name)
        </td>
        
    </tr>
}

</table>
